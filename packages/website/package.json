{
  "name": "website",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc && vite build",
    "build:staging": "tsc && env-cmd -f .env.staging vite build",
    "build:production": "tsc && env-cmd -f .env.production vite build",
    "preview": "vite preview",
    "test": "vitest",
    "format": "prettier --write .",
    "lint": "eslint .",
    "type-check": "tsc"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@mysten/sui.js": "^0.43.3",
    "@mysten/wallet-kit": "^0.7.7",
    "@reduxjs/toolkit": "^1.9.7",
    "@styled-icons/boxicons-logos": "^10.47.0",
    "animate.css": "^4.1.1",
    "axios": "^1.5.1",
    "env-cmd": "10.1.0",
    "gsap": "^3.12.2",
    "joi": "^17.11.0",
    "react": "^18.2.0",
    "react-collapse": "^5.1.1",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.16.0",
    "react-scroll": "^1.9.0",
    "sass": "^1.69.1",
    "styled-components": "^6.1.8",
    "swiper": "^11.0.7",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.27",
    "@types/react-collapse": "^5.0.4",
    "@types/react-dom": "^18.2.12",
    "@types/react-google-recaptcha": "^2.1.6",
    "@types/react-scroll": "^1.8.10",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.51.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-config-nick": "^1.0.2",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vitest": "^0.34.6"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "react/jsx-no-target-blank": "off"
    }
  },
  "prettier": "prettier-config-nick"
}
